{
  "manifest_version": 3,
  "name": "ZocDoc to Airtable Provider Extractor",
  "version": "1.0",
  "description": "Extract provider data from ZocDoc and send to Airtable",
  "permissions": ["storage", "activeTab"],
  "host_permissions": ["https://api.airtable.com/*"],
  "content_scripts": [{
    "matches": ["https://www.zocdoc.com/professional/*"],
    "js": ["content.js"]
  }],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Extract Provider Data"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png", 
    "128": "icon128.png"
  }
}
